@PrefixCls: commonlist;
.@{PrefixCls}-outer {

}
.@{PrefixCls}-item {
  & span {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }
  & img {
    width: 180px;
    height: 140px;
  }
}
.@{PrefixCls}-iframes {
  & iframe {
    width: 100px;
    height: 100px;
  }
}

