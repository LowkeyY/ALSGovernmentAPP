@PrefixCls: lanmutab;
.@{PrefixCls}-outer {

}
.@{PrefixCls}-item {
  & span {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }
  & img {
    width: 80px;
    height: 80px;
  }
}
.@{PrefixCls}-fixeditem {
  & img {
    width: 100px;
    height: 100px;
  }
}

